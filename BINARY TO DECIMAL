import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    System.out.println("Enter binary number: ");
	    String str=sc.nextLine();
	    int d=0;
	    int pow=0;
	    
	    for(int i=str.length()-1;i>=0;i--)
	    {
	       
	        if (str.charAt(i)=='1')
	        {
	            d+=Math.pow(2,pow);
	        }
	        pow++;
	    }
		System.out.println(d);
	}
}
